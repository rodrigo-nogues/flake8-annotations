{
  "problemMatcher": [
    {
      "owner": "pylint-error",
      "severity": "error",
      "pattern": [
        {
          "regexp": "^(.+?):([0-9]+): \\[([E|F][0-9]+)\\((.*)\\), (.*)\\] (.*)$",
          "file": 1,
          "line": 2,
          "code": 3,
          "message": 6
        }
      ]
    }
  ]
}
